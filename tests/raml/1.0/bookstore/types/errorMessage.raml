#%RAML 1.0 DataType
type: object
description: Standardized error message format
displayName: standardizedErrorMessage
properties:
  message:
    type: string
    required: true
  errors:
    type: string[]
    required: false
examples:
  standard: |
    {
      "message": "Invalid username or password."
    }
  missing: |
    {
      "message": "Invalid username or password.",
      "errors": [
        "The required parameter 'password' was not provided."
      ]
    }
